{
  "name": "highcharts-sonification-studio",
  "version": "0.0.24-beta",
  "description": "Edit and visualize data with charts and sound",
  "private": true,
  "scripts": {
    "test": "jest",
    "clean": "trash dist",
    "lint": "eslint --ext .ts,.js,.vue .",
    "dev": "npm run clean && npm run lint && parcel src/app/public/index.html --open",
    "build": "npm run clean && npm run lint && parcel build src/app/public/index.html --no-source-maps --public-url \"./\"",
    "copy-landing-page": "copyfiles -a -f \"./src/project_lander/*\" ./docs",
    "copy-to-docs": "cross-var \"trash docs/$npm_package_version && copyfiles -f \"./dist/**/*\" ./src/app/public/.nojekyll ./src/app/public/*.png ./src/app/public/favicon.ico ./src/app/public/manifest.webmanifest ./docs/$npm_package_version\"",
    "publish": " npm run build && npm run copy-to-docs && npm run copy-landing-page"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm test"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/highcharts/sonification-studio.git"
  },
  "keywords": [
    "sonification",
    "dataviz",
    "chart",
    "data"
  ],
  "author": "Highsoft AS",
  "license": "SEE LICENSE IN LICENSE.md",
  "bugs": {
    "url": "https://github.com/highcharts/sonification-studio/issues"
  },
  "homepage": "https://github.com/highcharts/sonification-studio#readme",
  "devDependencies": {
    "@types/jest": "^24.9.1",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "@vue/component-compiler-utils": "^3.1.2",
    "@vue/test-utils": "^1.0.3",
    "babel-core": "^7.0.0-bridge.0",
    "copyfiles": "^2.3.0",
    "cross-var": "^1.1.0",
    "eslint": "^6.8.0",
    "eslint-plugin-vue": "^6.2.2",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "less": "^3.11.3",
    "parcel-bundler": "^1.12.4",
    "sass": "^1.26.10",
    "trash-cli": "^3.0.0",
    "ts-jest": "^24.3.0",
    "typescript": "^3.9.6",
    "vue-jest": "^3.0.6",
    "vue-template-compiler": "^2.6.11"
  },
  "dependencies": {
    "@types/mathjs": "^6.0.5",
    "ag-grid-community": "^25.3.0",
    "ag-grid-vue": "^25.3.0",
    "highcharts": "^9.2.2",
    "highcharts-vue": "^1.3.5",
    "mathjs": "^7.1.0",
    "vue": "^2.6.11",
    "vue-hot-reload-api": "^2.3.4",
    "vue-property-decorator": "^8.5.1",
    "vue-router": "^3.5.1",
    "vuex": "^3.5.1"
  },
  "posthtml": {
    "recognizeSelfClosing": true
  }
}
